<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="613.0" prefWidth="754.0" styleClass="root" stylesheets="@../background/MyStyle.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.CustomerCartController">
   <children>
      <Label fx:id="titleTxt" layoutX="281.0" layoutY="14.0" textFill="#653030">
         <font>
            <Font name="Bell MT" size="36.0" />
         </font>
      </Label>
      <Label fx:id="bodyTxt" layoutX="25.0" layoutY="211.0" textFill="#821e1e">
         <font>
            <Font name="Bell MT" size="24.0" />
         </font>
      </Label>
      <Button fx:id="backBtn" layoutX="564.0" layoutY="53.0" mnemonicParsing="false" onAction="#backAction" prefHeight="43.0" prefWidth="155.0" styleClass="btn" stylesheets="@BtnStyle.css" text="Back" textFill="#680f0f" />
      <Button fx:id="CheckOutBtn" layoutX="564.0" layoutY="108.0" mnemonicParsing="false" onAction="#checkOutAction" prefHeight="43.0" prefWidth="155.0" styleClass="btn" stylesheets="@BtnStyle.css" text="Proceed To CheckOut" textFill="#680f0f" />
      <Button fx:id="deleteProductBtn" layoutX="45.0" layoutY="108.0" mnemonicParsing="false" onAction="#deleteProductAction" prefHeight="43.0" prefWidth="155.0" styleClass="btn" stylesheets="@BtnStyle.css" text="Delete Product" textFill="#680f0f" />
      <Label layoutX="19.0" layoutY="53.0" text="Enter product number to delete" textFill="#653030">
         <font>
            <Font name="Bell MT" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="numberTxt" layoutX="68.0" layoutY="73.0" prefHeight="25.0" prefWidth="108.0" />
      <Label fx:id="checkTxt" layoutX="18.0" layoutY="158.0" textFill="#653030">
         <font>
            <Font name="Bell MT" size="19.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
